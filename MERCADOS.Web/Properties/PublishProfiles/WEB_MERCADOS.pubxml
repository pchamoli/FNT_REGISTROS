            <Source Path="DBContext" DbContext="MERCADOS.Entidades.DB_REGISTROSEntities, MERCADOS.Contexto" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="DB_REGISTROSEntitiesUPC" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=localhost;Initial Catalog=DB_REGISTROS;User ID=sa;Password=Proyecto2016%" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\DB_REGISTROSEntitiesUPC_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="data source=localhost; initial catalog=DB_REGISTROS; user id=sa; password=Proyecto2016%" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
